<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data class="">

    </data>

    <FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
                 xmlns:app="http://schemas.android.com/apk/res-auto"
                 android:id="@+id/rl_photo_container"
                 android:layout_width="wrap_content"
                 android:layout_height="122px"
                 android:layout_marginLeft="@dimen/inspe_30px"
                 android:background="#00000000"
                 android:orientation="horizontal">

        <TextView
            android:id="@+id/txt_standard_type_name"
            android:layout_width="match_parent"
            android:layout_height="100px"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginRight="21px"
            android:layout_marginTop="21px"
            android:background="@drawable/inspe_light_blue_selector"
            android:gravity="center"
            android:minWidth="180px"
            android:paddingLeft="30px"
            android:paddingRight="30px"
            android:textColor="@color/colorPrimary"
            android:textSize="@dimen/inspe_38px"
            android:visibility="visible"
            app:layout_auto_baseheight="width"/>
    </FrameLayout>
</layout>