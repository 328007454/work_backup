<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#dde9e7">

    <View
        android:id="@+id/view_background"
        android:layout_width="match_parent"
        android:layout_height="@dimen/hwcw_line_height"
        android:background="#EFEFEF"
        />

    <TextView
        android:layout_below="@id/view_background"
        android:id="@+id/tv_panorama"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:gravity="center"
        android:paddingLeft="@dimen/hwcw_line_height"
        android:text="111111111"
        android:textColor="@color/app_base_green_color"
        android:textSize="@dimen/yw_title_gqj_size"
        />

    <ImageView
        android:id="@+id/iv_arrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/hwcw_line_height"
        android:layout_marginTop="25dp"/>
</RelativeLayout>
