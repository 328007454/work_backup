<?xml version="1.0" encoding="utf-8"?><!--android:layout_marginTop="@dimen/report_margin_top"-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_report_content_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/global_padding_top_bottom_px"
    android:background="@mipmap/ic_report_background"
    android:orientation="vertical"
    android:paddingLeft="@dimen/global_padding_half_left_right_px"
    android:paddingRight="@dimen/global_padding_half_left_right_px"
    android:paddingTop="@dimen/report_padding_top_px">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/global_padding_left_right_px"
        android:layout_marginRight="@dimen/global_padding_left_right_px"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="left|center_vertical"
            android:text="@string/work_start_time_str"
            android:textColor="@color/global_gray_text_color"
            android:textSize="@dimen/device_item_name_text_size_px" />

        <TextView
            android:id="@+id/start_time"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:gravity="left|center_vertical"
            android:textColor="@color/green_color"
            android:textSize="@dimen/global_text_size_px" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/global_padding_left_right_px"
        android:layout_marginRight="@dimen/global_padding_left_right_px"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="left|center_vertical"
            android:text="@string/work_end_time_str"
            android:textColor="@color/global_gray_text_color"
            android:textSize="@dimen/device_item_name_text_size_px" />

        <TextView
            android:id="@+id/end_time"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:gravity="left|center_vertical"
            android:textColor="@color/green_color"
            android:textSize="@dimen/global_text_size_px" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/global_padding_left_right_px"
        android:layout_marginRight="@dimen/global_padding_left_right_px"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="left|center_vertical"
            android:text="测试负责人："
            android:textColor="@color/global_gray_text_color"
            android:textSize="@dimen/device_item_name_text_size_px" />

        <TextView
            android:id="@+id/inspection_person"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:gravity="left|center_vertical"
            android:textColor="@color/green_color"
            android:textSize="@dimen/global_text_size_px" />
    </LinearLayout>
    <!--测试成员 -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/global_padding_left_right_px"
        android:layout_marginRight="@dimen/global_padding_left_right_px"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="left|center_vertical"
            android:text="测试成员："
            android:textColor="@color/global_gray_text_color"
            android:textSize="@dimen/device_item_name_text_size_px" />

        <TextView
            android:id="@+id/test_person"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:gravity="left|center_vertical"
            android:textColor="@color/green_color"
            android:textSize="@dimen/global_text_size_px" />
    </LinearLayout>

    <!--温度 -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/global_padding_left_right_px"
        android:layout_marginRight="@dimen/global_padding_left_right_px"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="left|center_vertical"
            android:text="蓄电池室内温度："
            android:textColor="@color/global_gray_text_color"
            android:textSize="@dimen/device_item_name_text_size_px" />

        <TextView
            android:id="@+id/temperature"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:gravity="left|center_vertical"
            android:textColor="@color/green_color"
            android:textSize="@dimen/global_text_size_px" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/global_padding_left_right_px"
        android:layout_marginRight="@dimen/global_padding_left_right_px"
        android:orientation="horizontal">


        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="left|center_vertical"
            android:text="@string/battery_test_type_str"
            android:textColor="@color/global_gray_text_color"
            android:textSize="@dimen/device_item_name_text_size_px" />

        <TextView
            android:id="@+id/test_type"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:gravity="left|center_vertical"
            android:textColor="@color/green_color"
            android:textSize="@dimen/global_text_size_px" />
    </LinearLayout>

    <com.cnksi.sjjc.View.DotView style="@style/DashStyle" />
    <!-- 检测结果-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="30px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="right|center_vertical"
            android:paddingRight="120px"
            android:text="巡检结果"
            android:textColor="@android:color/black"
            android:textSize="48px" />

        <TextView
            android:id="@+id/continue_inspection"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_vertical"
            android:layout_marginRight="@dimen/global_padding_left_right_px"
            android:background="@drawable/red_button_background_selector"
            android:clickable="true"
            android:drawableLeft="@mipmap/ic_continue_inspection"
            android:drawablePadding="@dimen/drawable_padding_px"
            android:gravity="center"
            android:paddingBottom="@dimen/text_button_padding_top_px"
            android:paddingLeft="@dimen/text_button_padding_left_px"
            android:paddingRight="@dimen/text_button_padding_left_px"
            android:paddingTop="@dimen/text_button_padding_top_px"
            android:text="@string/see_inspection_details_str"
            android:textColor="@android:color/white"
            android:textSize="@dimen/device_item_name_text_size_px" />
    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="20px"
        android:scrollbarStyle="outsideInset">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/layout_battery"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20px"
                android:layout_marginRight="20px"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/battery_group"
                    android:layout_width="match_parent"
                    android:layout_height="75px"
                    android:background="@color/battery_text_background"
                    android:gravity="center"
                    android:text="分析结论"
                    android:textColor="@color/battery_text_color"
                    android:textSize="48px"
                     />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="120px"
                    android:background="@color/battery_linearlayout_color"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txt_colusion"
                        style="@style/AnimalReportTextViewStyle"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="20px"
                        android:textColor="@android:color/black" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
