<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cnksi.inspe">

    <!-- SDCard中创建与删除文件权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <!-- 向SDCard写入数据权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/> -->

    <application
        android:name=".config.AppConfig"
        android:allowBackup="true"
        android:icon="@mipmap/inspe_ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/InspceAppTheme">
        <activity android:name=".ui.InspeLoadingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.InspeCreateActivity" />
        <activity android:name=".ui.InspeModifyActivity" />
        <activity android:name=".ui.InspeHtmlActivity" />
        <activity android:name=".ui.InspeIssueDetailActivity" />
        <activity android:name=".ui.InspeMainActivity" />
        <activity android:name=".ui.InspePlustekActivity" />
        <activity android:name=".ui.InspePlustekIssueListActivity" />
        <activity android:name=".ui.InspePlustekSimilarIssueActivity" />
        <activity android:name=".ui.InspePlustekIssueActivity" />
        <activity android:name=".ui.InspePlustekStandardActivity" />
        <activity android:name=".ui.InspeTeamActivity" />
        <activity android:name=".ui.InspeTeamIssueActivity" />
        <activity android:name=".ui.InspeTeamStandardActivity" />
        <activity android:name=".ui.InspeRecfityActivity" />
        <activity android:name=".ui.InspeDocActivity" />
        <activity android:name=".ui.InspeDeviceActivity" />
        <activity android:name=".ui.InspeDeviceDetailsActivity" />
    </application>

</manifest>