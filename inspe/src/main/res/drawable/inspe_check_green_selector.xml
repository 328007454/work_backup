<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!--选中-->
    <item android:state_enabled="true" android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="#05b1a3" />
            <corners android:radius="@dimen/btn_radius" />
            <stroke android:width="@dimen/btn_border_width1" android:color="#05b1a3" />
            <!--<size android:height="@dimen/btn_height" />-->
        </shape>
    </item>

    <!--默认-->
    <item android:state_enabled="true" android:state_checked="false">
        <shape android:shape="rectangle">
            <solid android:color="#0005b1a3" />
            <corners android:radius="@dimen/btn_radius" />
            <stroke android:width="@dimen/btn_border_width1" android:color="#C0BAB9" />
            <!--<size android:height="@dimen/btn_height" />-->
        </shape>
    </item>

    <!--不可用-->
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/btn_disable" />
            <corners android:radius="@dimen/btn_radius" />
            <stroke android:width="@dimen/btn_border_width1" android:color="#00FD5F54" />
            <!--<size android:height="@dimen/btn_height" />-->
        </shape>
    </item>


</selector>